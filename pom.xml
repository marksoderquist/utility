<?xml version="1.0" encoding="UTF-8"?>

<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

	<modelVersion>4.0.0</modelVersion>

	<parent>
		<groupId>com.parallelsymmetry</groupId>
		<artifactId>pom-parent-signed</artifactId>
		<version>1.0.0-SNAPSHOT</version>
	</parent>

	<groupId>com.parallelsymmetry</groupId>
	<artifactId>utility</artifactId>
	<version>1.0.1-SNAPSHOT-1</version>
	<packaging>jar</packaging>

	<name>Parallel Symmetry Utility Library</name>
	<description>This utility library contains useful classes that are generally useful for all types of applications.</description>
	<url>http://www.parallelsymmetry.com/hudson/job/utility/site/index.html</url>
	<inceptionYear>2007</inceptionYear>

	<scm>
		<connection>scm:svn:http://svn.parallelsymmetry.com/software/utility/trunk</connection>
	</scm>

	<repositories>
		<repository>
			<id>parallelsymmetry</id>
			<name>Parallel Symmetry</name>
			<url>http://mvn.parallelsymmetry.com/release</url>
			<snapshots>
				<enabled>false</enabled>
			</snapshots>
		</repository>
		<repository>
			<id>parallelsymmetry-snapshot</id>
			<name>Parallel Symmetry Snapshot</name>
			<url>http://mvn.parallelsymmetry.com/snapshot</url>
			<snapshots>
				<updatePolicy>always</updatePolicy>
			</snapshots>
		</repository>
	</repositories>

</project>
